{
  "name": "nestjs-lambda",
  "description": "Extension of the Nest.js framework for writing AWS lambda serverless functions",
  "version": "0.0.1-alpha.1",
  "main": "build/index.js",
  "license": "MIT",
  "homepage": "https://nestjs-lambda.com",
  "keywords": [
    "aws",
    "aws-lambda",
    "nestjs",
    "nestjs-library"
  ],
  "author": "Brandon Burrus <brandon@burrus.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/BrandonBurrus/nestjs-lambda.git"
  },
  "scripts": {
    "prebuild": "yarn clean",
    "prepublishOnly": "yarn build",
    "build": "tsc",
    "clean": "rimraf build"
  },
  "files": [
    "build"
  ],
  "jest": {
    "displayName": {
      "name": "nestjs-lambda",
      "color": "blue"
    },
    "roots": [
      "<rootDir>/tests"
    ],
    "testEnvironment": "node",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ]
  },
  "devDependencies": {
    "@nestjs/common": "^8.0.4",
    "@nestjs/core": "^8.0.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "gitHead": "5d60a9643d2a162c3e0781da7ccb05df0263acd4"
}
